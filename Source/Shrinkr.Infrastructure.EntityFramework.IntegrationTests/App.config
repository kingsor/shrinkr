<?xml version="1.0"?>
<configuration>
    <connectionStrings>
        <add
            name="Shrinkr"
            connectionString="Data Source=.\SQLExpress;Initial Catalog=Shrinkr;Integrated Security=SSPI;MultipleActiveResultSets=True"
            providerName="System.Data.SqlClient"
        />
    </connectionStrings>
    <startup>
        <requiredRuntime version="v4.0"/>
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.0"/>
    </startup>
</configuration>